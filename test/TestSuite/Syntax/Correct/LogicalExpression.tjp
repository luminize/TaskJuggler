project "LogExp" 2009-10-19 +2m {
  timezone "America/Denver"
}

resource r "R"
task "T" {
  allocate r
  effort 1d
}

macro rep [
taskreport  "LogExp${1}" {
  hidetask 1 ${2} 0
}
]

${rep "1" ">"}
${rep "2" "<"}
${rep "3" "<="}
${rep "4" ">="}
${rep "5" "!="}
${rep "6" "="}

taskreport "LogExp7" {
  hidetask @none
  hideresource @all
}
